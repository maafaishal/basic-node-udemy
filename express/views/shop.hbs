<main>
  <h1>My products</h1>
  <main>
    {{#if hasProducts}}
      <div class="container text-center">
        <div class="row">
          {{#each products}}
            <div class="col"><img src="https://picsum.photos/300/300" />
              <h1>{{this.title}}</h1>
            </div>
          {{/each}}
        </div>
      </div>
    {{else}}
      <h1>No Products!</h1>
    {{/if}}
  </main>
</main>